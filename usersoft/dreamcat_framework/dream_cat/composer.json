{
    "name": "business/answer.2345.com",
    "description": "答题项目",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "dreamcat/cat_frame_core": "^1@dev",
        "dreamcat/framework_swoole": "^1@dev",
        "ext-redis": "*",
        "ext-json": "*",
        "dreamcat/enhance_http_client": "^1",
        "ext-openssl": "*",
        "ext-amqp": "*",
        "dreamcat/apollo-php": "^1.0",
        "share_api/star": "^1.0",
        "share_api/real_cheat": "^1.0",
        "swiftmailer/swiftmailer": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "swoole/ide-helper": "^4.4",
        "kwn/php-rdkafka-stubs": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Activity\\Answer2345\\": "src/main/",
            "Dreamcat\\ApiDoc\\": "component/api_doc/src/main",
            "Dreamcat\\Procotol\\": "component/procotol/src/main",
            "Dreamcat\\Redis\\": "component/redis_factory/src/main",
            "Dreamcat\\PhpInner\\": "component/php_inner/src/main",
            "Dreamcat\\AssistTools\\": "component/assist_tools/src/main",
            "Activity\\Answer2345Api\\Common\\": "answerapi/common/src/main/",
            "Activity\\Answer2345Api\\Operation\\": "answerapi/operationapi/src/main/",
            "Dreamcat\\RabbitMQ\\": "component/rabbit_mq/src/main",
            "Dreamcat\\RabbitMQPool\\": "component/rabbit_mq_pool/src/main",
            "Dreamcat\\RedisPool\\": "component/redis_factory_pool/src/main"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Activity\\Answer2345\\": "src/test/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "secure-http": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.2345.com/repo/private/"
        }
    ]
}
